<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="collective.quickupload">
  <body>
    <metal:content-description fill-slot="content-description">
      <metal:description define-macro="content-description">
        <!-- no description -->
      </metal:description>
    </metal:content-description>
    <metal:content-core fill-slot="content-core">
      <metal:content-core define-macro="content-core">

      <script  type="text/javascript" tal:content="view/javascript">
      </script>
      <div class="quickupload uploaderContainer">
          <input type="hidden"
                 name="uploadUrl"
                 class="uploadUrl"
                 tal:attributes="value view/get_upload_url" />
      </div>

      </metal:content-core>
    </metal:content-core>

  </body>
</html>
