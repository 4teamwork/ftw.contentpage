            <title>Default table columns</title>
            <required>True</required>
			<value_type type="plone.registry.field.TextLine" />
        </field>
        <value>
	        <element>getContentType</element>
	        <element>Title</element>
	        <element>modified</element>
		</value>
    </record>
    <record name="ftw.contentpage.addressblock.defaulttitle">
        <field type="plone.registry.field.TextLine">
            <title>Default title of a listingblock</title>
            <required>False</required>
        </field>
        <value>Address</value>
    </record>
</registry>
